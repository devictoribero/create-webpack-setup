{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
    "valid-jsdoc": ["error", {
      "requireReturn": true,
      "requireReturnType": true,
      "requireParamDescription": true,
      "requireReturnDescription": true,
    }],
    "no-underscore-dangle": 0,
    "require-jsdoc": 0,
    "no-tabs": 0,
    "no-use-before-define": 0
  }
}
